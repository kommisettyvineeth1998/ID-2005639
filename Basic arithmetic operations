#include<stdio.h>
int a=60,b=20;
int s=0, d=0, m=0, q=0;

int main()
{
s= a+b;
printf("Sum of two numbers = %d\n",Sum);

d=a-b;
printf("Difference of two numbers = %d\n",Diff);

m=a*b;
printf("Multiplication of two numbers = %d\n",Mul);

q=a/b;
printf("Multiplication of two numbers = %d\n",Div);

return 0;
}
